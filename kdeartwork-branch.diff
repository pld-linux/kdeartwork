diff -urN -x CVS kdeartwork.orig/debian/clean-sources kdeartwork/debian/clean-sources
--- kdeartwork.orig/debian/clean-sources	Thu Jan  1 01:00:00 1970
+++ kdeartwork/debian/clean-sources	Thu Oct 14 14:06:18 2004
@@ -0,0 +1,18 @@
+#!/bin/sh
+set -e
+
+if [ ! -d kworldclock ]; then
+  echo "You must be in the top-level kdeartwork directory."
+  exit 1
+fi
+
+
+echo "Processing non-DFSG-free kdeartwork data..."
+imgdirs=kworldclock/depths
+
+if [ ! -e "$imgdirs" ]; then
+  echo "Non-free images not found, perhaps you have already cleaned them out?"
+else
+  rm -rf $imgdirs
+  echo "Removed."
+fi
